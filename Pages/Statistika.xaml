<UserControl x:Class="ISNemocniceKlient.Pages.Statistika"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="600" Loaded="UserControl_Loaded">
    <Grid Style="{StaticResource ContentRoot}">
        <ScrollViewer>
            <StackPanel MinWidth="200">
                <TextBlock Text="Statistika nemocnice" Style="{StaticResource Heading1}" />
                <mui:BBCodeBlock Name="bbCodeBlockStatistikyNemocnice" BBCode="..." />
                <TextBlock Text="Přihlášený účet" Style="{StaticResource Heading1}" />
                <mui:BBCodeBlock Name="bbCodeBlockStatistikyPrihlasenyUcet" BBCode="..." />
            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
